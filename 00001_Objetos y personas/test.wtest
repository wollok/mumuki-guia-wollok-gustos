describe "Rosa"{
	test "le gusta" {
		assert.that(rosa.leGusta(pelota))
		assert.that(rosa.leGusta(remera))
		
		munieco.peso(1500)
		assert.that(rosa.leGusta(munieco))
	}
	test "no le gusta" {
		assert.notThat(rosa.leGusta(biblioteca))
		
		placa.peso(3500)
		assert.notThat(rosa.leGusta(placa))
	}
}